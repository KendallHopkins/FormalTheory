{
    "name": "witrin/formal-theory",
    "type": "library",
    "description": "FormalTheory is a library that allows for manipulation and conversion of NFAs, DFAs and Regular Expressions.",
    "version": "1.1.0",
    "keywords": [
        "formal",
        "theory",
        "regex",
        "regular",
        "expression",
        "dfa",
        "nfa",
        "fa",
        "deterministic",
        "nondeterministic",
        "finite",
        "automata",
        "lexer",
        "transform",
        "optimizer",
        "state"
    ],
    "homepage": "https://github.com/witrin/FormalTheory",
    "license": "MIT",
    "authors": [
        {
            "name": "Kendall Hopkins",
            "email": "softwareelves@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "FormalTheory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FormalTheory\\Tests\\": "tests/"
        }
    }
}
